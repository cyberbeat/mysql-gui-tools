@echo off
rem This cleans all files that were generated by MakeSetupGpl.bat
rem   or by MakeSetupCommercial.bat (including the .msi and .msm
rem   file)

echo Cleaning the files in setup\windows\...
cd ..\setup\windows\
nmake /NOLOGO -f Makefile.mak clean
cd ..\..\build\

echo Cleaning the files in bin\dist\
del ..\bin\dist\*.msi
del ..\bin\dist\*.msm

echo All files deleted.
pause